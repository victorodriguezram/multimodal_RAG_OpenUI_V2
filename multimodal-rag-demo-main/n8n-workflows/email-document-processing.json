      "typeVersion": 2,
      "position": [880, 300]
    },
    {
      "parameters": {
        "url": "http://your-server.com/api/n8n/webhook/upload",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "httpHeaderAuth",
        "sendBinaryData": true,
        "binaryPropertyName": "data",
        "options": {
          "bodyContentType": "multipart-form-data",
          "formData": {
            "user_id": "email_processor_user",
            "webhook_secret": "={{ $env.N8N_WEBHOOK_SECRET }}"
          }
        }
      },
      "id": "8e1f2g56-7890-1234-efg1-567890123456",
