      "credentials": {
        "httpHeaderAuth": {
          "id": "2",
          "name": "RAG API Key"
        }
      }
    },
    {
      "parameters": {
        "operation": "getAll",
        "base": {
          "__rl": true,
          "value": "appXXXXXXXXXXXXXX",
          "mode": "list",
          "cachedResultName": "Document Processing Log"
        },
        "table": {
          "__rl": true,
          "value": "tblXXXXXXXXXXXXXX",
          "mode": "list",
          "cachedResultName": "Processing Log"
        },
        "options": {}
      },
      "id": "9f2g3h67-8901-2345-fgh2-678901234567",
