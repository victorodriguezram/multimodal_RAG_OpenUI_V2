{
  "name": "Document Processing from Email",
  "nodes": [
    {
      "parameters": {
        "pollTimes": {
          "item": [
            {
              "mode": "everyMinute"
            }
          ]
        },
        "format": "simple",
        "options": {
          "allowUnauthorizedCerts": false,
          "port": 993,
          "secure": true,
          "markSeen": true
        },
        "mailbox": "INBOX",
